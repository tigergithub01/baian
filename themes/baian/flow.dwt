  function selectAll(obj, chk)
{
  if (chk == null)
  {
      chk = 'checkboxes';
  }
  var elems = obj.form.getElementsByTagName("INPUT");
  for (var i=0; i < elems.length; i++)
  {
    if (elems[i].name == chk || elems[i].name == chk + "[]")
    {
      elems[i].checked = obj.checked;
    }
  }
} <!--add by wanggz可以从购物车中选择商品购买功能end--> 
 
