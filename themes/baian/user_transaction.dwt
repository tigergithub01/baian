					document.getElementById("tuihuokuan").innerHTML = '<table width="100%" border="0"><tr><td bgcolor="#ffffff">退货理由</td><td colspan="5" bgcolor="#ffffff" style="padding-left:70px"><select name="liyou"><option value="">请选择退货原因</option><option value="大小尺寸不符">大小尺寸不符</option><option value="材质、面料不符">材质、面料不符</option><option value="工艺、手艺问题">工艺、手艺问题</option><option value="颜色、款式、图案与描述不符">颜色、款式、图案与描述不符</option><option value="不喜欢，穿着效果不好">不喜欢，穿着效果不好</option><option value="其他">其他</option></select><font color="#FF0000">*</font></td></tr></table>';
					txt = "退货";
				}else{
					document.getElementById("shuoming").innerHTML = '退款说明';
					document.getElementById("tuihuokuan").innerHTML  = '<table width="100%" border="0"><tr><td bgcolor="#ffffff">退款理由</td><td colspan="5" bgcolor="#ffffff" style="padding-left:70px"><select name="liyou"><option value="">请选择退款原因</option><option value="客服通知说断货下架了">客服通知说断货下架了</option><option value="暂时断货，需要几天才到货，我不想等了">暂时断货，需要几天才到货，我不想等了</option><option value="其他">其他</option></select><font color="#FF0000">*</font></td></tr></table>';
					txt = "退款";
				}
			}
	   		var pase = false;
	   		function checkBack(){
				var liyou = document.back_order.liyou.value;
				var beizhu = document.back_order.beizhu.value;
				
				if(liyou == ""){
					alert("请选择"+txt+"理由！");	
					document.back_order.liyou.focus();
					return false;
				}
				if(beizhu == ""){
					alert("请填写"+txt+"说明！");	
					document.back_order.beizhu.focus();
					return false;
				}else{
					if(beizhu.length > 196){
						alert(txt+"说明文字不能多于196个！");	
						document.back_order.beizhu.focus();
						return false;
					}
				}
			
				var j = true;
				var k = 0;
				var l = true;
				for(i=0;i<total;i++){
					if(document.getElementById("cbox"+i).checked){
						k = k + 1;							

	
					}
					if(document.getElementById("cbox"+i).checked && 

(document.getElementById("putnum"+i).value == "" || parseInt(document.getElementById("putnum"+i).value) == 0)){
						j = false;
					}
					if(!document.getElementById("cbox"+i).checked && parseInt

(document.getElementById("putnum"+i).value) > 0){
						l = false;
					}
				}
				if(k == 0){
					alert("请至少选择一个要"+txt+"的商品");	
					return false;						
				}				
				if(!j){
					alert("请填写"+txt+"商品的数量！");	
					return false;						
				}	
				if(!l){
					alert("您选择了"+txt+"商品的数量，请选择对应的商品！");	
					return false;						
				}					
				if(!pase){
					alert(txt+"数量不能超过购买数量！");	
					return false;						
				}				
			}
			
			function checkNum(key,num){
				if(parseInt(document.getElementById("number"+key).innerHTML) < num){
					alert(txt+"数量不能超过购买数量！");
					pase = false;
					return false;						
				}else{
					pase = true;	
				}
			}
